[alias]
	l = log --pretty=oneline -n 20 --graph
	plog = log --graph --pretty=format:'%C(yellow)%h%Creset%C(blue)%d%Creset %C(white bold)%s%Creset %C(white dim)(by %an %ar)%Creset' --all
	s = status -s
	go = checkout -B
[user]
	name = Matt Stills
	email = mattacular@gmail.com
[credential]
	helper = cache
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[merge]
	log = true

